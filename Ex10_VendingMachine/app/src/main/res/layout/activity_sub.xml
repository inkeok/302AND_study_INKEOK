    <TextView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="자판기 잔돈 반환"/>
    <TextView
        android:id="@+id/text1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="잔액"/>
    <TextView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="주문음료 목록"/>
    <TextView
       android:id="@+id/text2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="몇개"/>

</LinearLayout>